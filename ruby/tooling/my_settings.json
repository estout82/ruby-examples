{
  "debug.allowBreakpointsEverywhere": true,
  "explorer.confirmDragAndDrop": false,
  "testExplorer.addToEditorContextMenu": true,
  "explorer.confirmDelete": false,
  "C_Cpp.updateChannel": "Insiders",
  "[cpp]": {
      
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorTheme": "Atom One Dark",
  "AllAutocomplete.showCurrentDocument": false,
  "[plaintext]": {
  "editor.quickSuggestions": false,
  "editor.wordBasedSuggestions": false,
  "editor.suggest.showClasses": false,
  "editor.suggest.showConstants": false,
  "editor.suggest.showColors": false,
  "editor.suggest.showCustomcolors": false,
  "editor.suggest.showConstructors": false,
  "editor.suggest.showEnums": false,
  "editor.suggest.showEnumMembers": false,
  "editor.suggest.showFields": false,
  "editor.suggest.showEvents": false,
  "editor.suggest.showFiles": false,
  "editor.suggest.showFolders": false
  },
  "security.workspace.trust.untrustedFiles": "open",
  "mssql.connections": [],
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "editor.tabSize": 2,
  "window.zoomLevel": 2,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 500,
  "editor.wordBasedSuggestions": false,

  "ruby.interpreter.commandPath": "/usr/bin/env ruby",
  "ruby.useBundler": true,
  "ruby.useLanguageServer": true,
  "ruby.lint": {
    "rubocop": {
      "useBundler": true
    },
    "reek": {
      "useBundler": true
    }
  },
  "ruby.format": "rubocop",
  "scssLint.showHighlights": false,
    "scssLint.runOnTextChange": false,
    "scssLint.errorBackgroundColor": "rgba(200, 0, 0, .8)",
    "scssLint.warningBackgroundColor": "rgba(200, 120, 0, .8)",
    "scssLint.languages": [
        "scss"
    ],
    "scssLint.statusBarText": "`$(telescope) scss-lint  ${errors.length} $(x)  ${warnings.length} $(alert)`",
    "scssLint.configDir": ""
}